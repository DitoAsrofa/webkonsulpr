{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    },
    {
      "src": "/",
      "dest": "api/index.php"
    },
    {
      "src": "/landing.php",
      "dest": "api/landing.php"
    },
    {
      "src": "/login.php",
      "dest": "api/login.php"
    },
    {
      "src": "/dashboard.php",
      "dest": "api/dashboard.php"
    }
  ]
}